[Unit]
Description=Frontend
After=network.target

[Service]
Restart=always
RestartSec=3

User={{service.user}}
Group={{service.user}}

AmbientCapabilities=CAP_NET_BIND_SERVICE
ExecStart=http-server {{service.public}} -p {{service.port}} --proxy http://localhost:8080

[Install]
WantedBy=multi-user.target
